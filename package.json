{
  "name": "next-tailwind-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "eslint ./**/*.{ts,tsx,js}",
    "format": "prettier --write ./**/*.{ts,tsx,css,md,mdx}",
    "type-check": "tsc --pretty --noEmit",
    "husky:install": "husky install",
    "prepare": "is-ci-cli \"\" \"husky:install\"",
    "test": "is-ci-cli \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --watch=false --coverage"
  },
  "dependencies": {
    "@headlessui/react": "^1.3.0",
    "@heroicons/react": "^1.0.1",
    "@prisma/client": "2.26.0",
    "@tailwindcss/forms": "^0.3.3",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.1.1",
    "next": "^11.0.1",
    "next-auth": "^3.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.18.1",
    "redaxios": "^0.4.1",
    "zod": "^3.3.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/tailwindcss": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.0.4",
    "babel-jest": "^27.0.6",
    "eslint": "^7.25.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-hooks": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "husky": ">=6",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^3.0.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": ">=10",
    "postcss": "^8.1.10",
    "prettier": "^2.2.1",
    "prisma": "2.26.0",
    "tailwindcss": "^2.1.1",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "yarn lint --fix",
      "yarn format"
    ]
  }
}
