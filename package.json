    "@tanstack/react-query": "^5.32.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.30.9",
    "next": "^14.2.3",
    "pg": "^8.11.5",
    "postgres": "^3.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
