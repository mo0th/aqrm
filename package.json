    "@types/node": "^20.12.7",
    "@types/pg": "^8.11.5",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/tailwindcss": "^3.1.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.17",
    "eslint": "^9.1.1",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
