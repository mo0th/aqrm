    "is-ci": "^3.0.1",
    "is-ci-cli": "^2.2.0",
    "lint-staged": ">=15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "terser": "^5.30.4",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1"
